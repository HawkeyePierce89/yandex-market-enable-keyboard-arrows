{
    "manifest_version": 2,
    "name": "yandex-market-enable-keyboard-arrows",
    "description": "Enable keyboard arrows for compare on Yandex.Market",
    "version": "1.0.0",
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "content_scripts": [
        {
            "matches": ["https://market.yandex.ru/*"],
            "js": ["content.js"]
        }
    ]
}
